<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app>
    <servlet>
        <servlet-name>
            parserupload
        </servlet-name>
        <servlet-class>
            UploadServlet
        </servlet-class>
        <init-param>
            <param-name>
                uploadDir
            </param-name>
            <param-value>
                /export/jgenerator/2.1/tomcat/webapps/ROOT/upload/
            </param-value>
        </init-param>
        <init-param>
            <param-name>
                targetAppFieldName
            </param-name>
            <param-value>
                targetApp
            </param-value>
        </init-param>
        <init-param>
            <param-name>
                targetFieldFieldName
            </param-name>
            <param-value>
                targetField
            </param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>
            echoservlet
        </servlet-name>
        <servlet-class>
            EchoServlet
        </servlet-class>
    </servlet>
    <taglib>
        <taglib-uri>http://jzox.com/swf</taglib-uri>
        <taglib-location>/WEB-INF/swf.tld</taglib-location>
    </taglib>
</web-app>