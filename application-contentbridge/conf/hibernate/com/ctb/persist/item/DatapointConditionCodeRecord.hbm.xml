    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class
        name="com.ctb.persist.item.DatapointConditionCodeRecord"
        table="DATAPOINT_CONDITION_CODE"
