<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
    "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
    <servlet>
        <servlet-name> parserupload </servlet-name>
        <servlet-class> com.ctb.servlet.UploadServlet </servlet-class>
    </servlet>
    <servlet>
        <servlet-name> Capture </servlet-name>
        <servlet-class> com.ctb.servlet.CaptureStreamServlet </servlet-class>
    </servlet>
    <servlet>
        <servlet-name> PdfXmlAk </servlet-name>
        <servlet-class> com.ctb.servlet.PDFXmlAKServlet </servlet-class>
    </servlet>
    <servlet>
        <servlet-name> PdfXml </servlet-name>
        <servlet-class> com.ctb.servlet.PDFXmlServlet </servlet-class>
    </servlet>
    <servlet>
        <servlet-name> FlashXml </servlet-name>
        <servlet-class> com.ctb.servlet.FlashItemTempServlet </servlet-class>
    </servlet>
    <servlet>
        <servlet-name> FlashAssessment </servlet-name>
        <servlet-class> com.ctb.servlet.FlashAssessmentAKServlet </servlet-class>
    </servlet>
    <servlet>
        <servlet-name> FlashItem </servlet-name>
        <servlet-class> com.ctb.servlet.FlashItemAKServlet </servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name> PdfXmlAk </servlet-name>
        <url-pattern> /pdfakxml </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name> PdfXmlAk </servlet-name>
        <url-pattern> /xmlanswrkey </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name> PdfXml </servlet-name>
        <url-pattern> /pdfxml </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name> PdfXml </servlet-name>
        <url-pattern> /fopxml </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name> FlashXml </servlet-name>
        <url-pattern> /flashxml </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name> FlashAssessment </servlet-name>
        <url-pattern> /flashassessment </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name> FlashAssessment </servlet-name>
        <url-pattern> /flashsubtest </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name> FlashAssessment </servlet-name>
        <url-pattern> /flashitemset </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name> FlashItem </servlet-name>
        <url-pattern> /flashitem </url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name> Capture </servlet-name>
        <url-pattern> /capturemime </url-pattern>
    </servlet-mapping>

</web-app>
