<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSPY v5 rel. 2 U (http://www.xmlspy.com) by nanjappan erode (private) -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tms="http://www.ctb.com/tms" elementFormDefault="qualified" attributeFormDefault="unqualified" >
<!-- <xs:import namespace="http://www.ctb.com/tms" schemaLocation="file:///C:/workspace103/Schemas/schemas/sds-to-tms.xsd"/>-->
<xs:include schemaLocation="sds-to-tms.xsd" />
	<xs:simpleType name="stereotypeType">
		<xs:annotation>
			<xs:documentation>Enumeration of parts of an item.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="directions"/>
			<xs:enumeration value="stimulus"/>
			<xs:enumeration value="stem"/>
			<xs:enumeration value="answerArea"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="tmssvc_request">
		<xs:annotation>
			<xs:documentation>Request a method of the Test Management service (TMS) web service</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="login_request">
					<xs:annotation>
						<xs:documentation>
							Message containing the login data from the
							examinee workstation
						</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="user_name" use="required">
							<xs:annotation>
								<xs:documentation>
									User name supplied by the student at
									the login.
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern value="([a-zA-Z0-9])*" />
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="password" use="required">
							<xs:annotation>
								<xs:documentation>
									Password supplied by the student at
									the time of login.Pattern is a-z,
									A-Z, 0-9,special charactersExample
									abc@lt;@quot;@amp;
								</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:pattern
										value="([a-zA-Z0-9@lt;@quot;@amp;])*" />
								</xs:restriction>
							</xs:simpleType>
						</xs:attribute>
						<xs:attribute name="access_code"
							type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation>
									Access Code supplied by the student
									at the time of login. This access
									code would be communicated to
									student prior to the test
								</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="is_reopen" type="xs:string" use="optional">
							<xs:annotation>
								<xs:documentation>is_reopen is  a boolean flag to indicated where the test client supports student with more than one intrupted state</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="os_enum"
							type="operatingSystemType" use="optional">
							<xs:annotation>
								<xs:documentation>
									Enumerated name for the operating
									system of the examinee workstation
								</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="browser_agent_enum"
							type="browserAgentType" use="optional">
							<xs:annotation>
								<xs:documentation>
									Enumerated name for the browser
									agent of the examinee workstation
								</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="user_agent_string"
							type="xs:string" use="optional">
							<xs:annotation>
								<xs:documentation>
									User-agent string retrieved from the
									Http Header. For example Mozilla/4.0
									(compatible; MSIE 6.0; Windows NT
									5.0)
								</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="sds_date_time"
							type="xs:dateTime" use="optional">
							<xs:annotation>
								<xs:documentation>
									Server Date and Time stamp. The SDS
									adds this time stamp when it
									receives the login data message from
									the examinee workstation.
								</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="sds_id" type="xs:string"
							use="optional" />
						<xs:attribute name="token" type="xs:string"
							use="required" />
					</xs:complexType>
				</xs:element>

				<xs:element name="runLoad_request">
					<xs:complexType>

						<xs:attribute name="system_id"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="system_time"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name ="network_service"		
						type ="xs:string">
						 </xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="upload_statistics_request">
					<xs:complexType>
						<xs:attribute name="system_id"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="roster_id"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="max_response_time"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="min_response_time"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="avg_response_time"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="success_count"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="failure_count"
							type="xs:string">
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="upload_systemInfo_request">
					<xs:complexType>
						<xs:attribute name="system_id"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="os_name" type="xs:string">
						</xs:attribute>
						<xs:attribute name="os_version"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="system_model"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="physical_memory"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="virtual_memory"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="processors"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="network_cards"
							type="xs:string">
						</xs:attribute>		
					</xs:complexType></xs:element>
			</xs:sequence>
			<xs:attribute name="method" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="browserAgentType">
		<xs:annotation>
			<xs:documentation>Type for the browser enumeration </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="MSIE"/>
			<xs:enumeration value="Mozilla"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="operatingSystemType">
		<xs:annotation>
			<xs:documentation>Type for the operating system enumeration</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Windows"/>
			<xs:enumeration value="Mac"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="tmssvc_response">
		<xs:annotation>
			<xs:documentation>Response method of the Test Management Service (TMS) web service</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="login_response">
					<xs:annotation>
						<xs:documentation>
							Message containing the login response data
							from the TCM
						</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="status">
								<xs:complexType>
									<xs:attribute name="status_code"
										use="required">
										<xs:simpleType>
											<xs:restriction
												base="xs:string">
												<xs:enumeration
													value="200">
													<xs:annotation>
														<xs:documentation>
															OK. This
															means that
															the TMS
															successfully
															handles the
															request
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="400">
													<xs:annotation>
														<xs:documentation>
															Bad Request.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="450">
													<xs:annotation>
														<xs:documentation>
															Authentication
															Required.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="451">
													<xs:annotation>
														<xs:documentation>
															Missing
															Information.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="452">
													<xs:annotation>
														<xs:documentation>
															Invalid
															Data.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="461">
													<xs:annotation>
														<xs:documentation>
															This
															application
															is out of
															date. Please
															visit
															http://iknow.ctb.com/
															to obtain
															the latest
															version.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="471">
													<xs:annotation>
														<xs:documentation>
															One or more
															of the
															following
															errors has
															occurred:
															Your
															username is
															not found,
															the access
															code is not
															found, you
															are not
															assigned to
															the test
															session, or
															your
															password
															does not
															match.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="472">
													<xs:annotation>
														<xs:documentation>
															You already
															have a test
															session in
															progress.
															You cannot
															have more
															than one
															session
															opened.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="473" />
												<xs:enumeration
													value="474">
													<xs:annotation>
														<xs:documentation>
															The test
															session is
															not
															available at
															this time.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="475">
													<xs:annotation>
														<xs:documentation>
															You cannot
															log into
															this test
															session
															because you
															have already
															completed
															it.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="476">
													<xs:annotation>
														<xs:documentation>
															You cannot
															log into
															this test
															session
															because your
															answers have
															already been
															entered. See
															your proctor
															for help.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="477">
													<xs:annotation>
														<xs:documentation>
															You cannot
															log into
															this test
															session
															because your
															locator
															subtest is
															not
															completed.
															See your
															proctor for
															help.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="500">
													<xs:annotation>
														<xs:documentation>
															An internal
															server error
															has
															occurred.
															Contact your
															proctor.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="501">
													<xs:annotation>
														<xs:documentation>
															A server
															error has
															occurred.
															Contact your
															proctor.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
												<xs:enumeration
													value="560">
													<xs:annotation>
														<xs:documentation>
															Systems are very busy. For optimal testing experience, please try again at a later time.
														</xs:documentation>
													</xs:annotation>
												</xs:enumeration>
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
							<xs:element name="ambiance" minOccurs="0">
								<xs:annotation>
									<xs:documentation>
										Preliminary information. For
										discussion only. Accommodate
										"look" for different product.
									</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="logo"
											minOccurs="0" />
										<xs:element
											name="stereotype_style" minOccurs="0">
											<xs:complexType>
												<xs:attribute
													name="bgcolor" type="xs:string" use="required">
													<xs:annotation>
														<xs:documentation>
															Background
															color for
															this
															stereotype.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="font_color" type="xs:string" use="required">
													<xs:annotation>
														<xs:documentation>
															Font color
															for this
															stereotype.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="testing_session_data">
								<xs:annotation>
									<xs:documentation>
										Student Demographic and
										preference data
									</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="cmi.core">
											<xs:annotation>
												<xs:documentation>
													Student name and id
												</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute
													name="student_id" type="xs:string">
													<xs:annotation>
														<xs:documentation>
															Unique
															alpha-numeric
															code that
															refers to
															the single
															user in the
															system
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="student_last_name" type="xs:string">
													<xs:annotation>
														<xs:documentation>
															This is the
															last name of
															the student.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="student_first_name" type="xs:string">
													<xs:annotation>
														<xs:documentation>
															This is the
															first name
															of the
															student
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="student__middle_name" type="xs:string">
													<xs:annotation>
														<xs:documentation>
															This is the
															middle name
															of the
															student.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element
											name="lms.student.accommodations" minOccurs="0">
											<xs:annotation>
												<xs:documentation>
													This is only
													required if the
													student needs
													accommodations
												</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element
														name="stereotype_style" minOccurs="0" maxOccurs="4">
														<xs:complexType>
															<xs:attribute
																name="stereotype" type="stereotypeType"
																use="required">
																<xs:annotation>
																	<xs:documentation>
																		Part
																		of
																		the
																		screen
																		to
																		apply
																		this
																		style.
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute
																name="bgcolor" type="xs:string" use="required">
																<xs:annotation>
																	<xs:documentation>
																		Background
																		color
																		for
																		this
																		stereotype.
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute
																name="font_color" type="xs:string" use="required" />
															<xs:attribute
																name="font_magnification" type="xs:float"
																use="required" />
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute
													name="calculator" type="xs:boolean" use="optional"
													default="0">
													<xs:annotation>
														<xs:documentation />
														<xs:documentation>
															Indicates
															whether the
															calculator
															is required
															or not by
															the special
															accommodation
															student.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="magnifier" type="xs:boolean" use="optional"
													default="0">
													<xs:annotation>
														<xs:documentation>
															Indicates
															whether the
															magnifier is
															required or
															not by the
															special
															accommodation
															student.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="screen_reader" type="xs:boolean" use="optional"
													default="0">
													<xs:annotation>
														<xs:documentation>
															Indicates
															whether the
															screen
															reader is
															required or
															not by the
															special
															accommodation
															student.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="untimed" type="xs:boolean" use="optional"
													default="0">
													<xs:annotation>
														<xs:documentation>
															Indicates
															whether the
															test is
															timed or not
															for the
															special
															accommodations
															student.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="rest_break" type="xs:boolean" use="optional"
													default="0" />
												<xs:attribute
													name="masking_ruler" type="xs:boolean" use="optional"
													default="0" />
												<xs:attribute
													name="highlighter" type="xs:boolean" use="optional"
													default="0">
													<xs:annotation>
														<xs:documentation>
															Indicates
															whether or
															not the
															highlighter
															manipulative
															is to be
															available
															for the
															student.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="tutorial">
								<xs:annotation>
									<xs:documentation>
										Student tutorial information
									</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:attribute
										name="deliver_tutorial" type="xs:integer" use="required" />
									<xs:attribute name="tutorial_url"
										type="xs:string" use="optional" />
								</xs:complexType>
							</xs:element>
							<xs:element name="manifest">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="sco"
											maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>
													One or more subtest
													related data with in
													a test
												</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="id"
													type="xs:string" use="required">
													<xs:annotation>
														<xs:documentation>
															SCO ID. This
															is the
															globally
															unique ID of
															the
															sub-test.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="adsid" type="xs:string" use="optional">
													<xs:annotation>
														<xs:documentation>
															ADS object
															bank id for
															subtest
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="title" type="xs:string" use="required">
													<xs:annotation>
														<xs:documentation>
															The title of
															the sub-test
															to be
															displayed on
															the user
															interface.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="cmi.core.entry" type="entryType" use="required" />
												<xs:attribute
													name="sco_duration_minutes" type="xs:integer"
													use="required">
													<xs:annotation>
														<xs:documentation>
															This is the
															duration of
															the SCO in
															minutes.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="sco_unit_type" use="required">
													<xs:annotation>
														<xs:documentation>
															Indicates
															the type of
															the SCO.
														</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction
															base="xs:string">
															<xs:enumeration
																value="SUBTEST" />
															<xs:enumeration
																value="SECTION" />
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
												<xs:attribute
													name="sco_unit_question_number_offset" type="xs:string"
													use="optional" default="0">
													<xs:annotation>
														<xs:documentation>
															This number
															indicates
															the offset
															to be added
															to the
															question
															numbers
															displayed to
															the student
															for this sco
															unit. For
															example if
															the offset
															is 10, then
															for this
															Ã¢â‚¬Å“sectionÃ¢â‚¬Â�
															with 20
															questions,
															the question
															bar will
															diplsay
															question
															numbers from
															10 to 19
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="force_logout" type="xs:boolean" use="optional"
													default="0">
													<xs:annotation>
														<xs:documentation>
															Indicates
															that the
															student must
															be forced to
															logout AFTER
															this SCO is
															completed.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="cmi.core.total_time" type="xs:string"
													use="optional">
													<xs:annotation>
														<xs:documentation>
															The amount
															of time that
															the student
															has already
															spent in
															this
															sub-test
															totalled
															across all
															sessions
															(restarts)
															specified as
															"HH:MM:SS".
															Required
															only for
															restart.
															Referer to
															SCORM v1.2.
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="asmt_hash" type="xs:string" use="optional">
													<xs:annotation>
														<xs:documentation>
															subtest
															checksum
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="asmt_encryption_key" type="xs:string"
													use="optional">
													<xs:annotation>
														<xs:documentation>
															for subtest
															decryption
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
												<xs:attribute
													name="item_encryption_key" type="xs:string"
													use="optional">
													<xs:annotation>
														<xs:documentation>
															for item and
															asset
															decryption
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
										<xs:element name="feedback">
											<xs:annotation>
												<xs:documentation>
													Student Feedback
												</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="id"
													type="xs:string" use="required" />
											</xs:complexType>
										</xs:element>
										<xs:element name="terminator">
											<xs:annotation>
												<xs:documentation>
													End of the manifest.
												</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:attribute name="id"
													type="xs:string" use="required">
													<xs:annotation>
														<xs:documentation>
															Currently
															the only
															terminator
															allowed is
															"SEE_YOU_LATER".
														</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="title"
										type="xs:string" use="required" />
								</xs:complexType>
							</xs:element>
							<xs:element
								name="consolidated_restart_data">
								<xs:annotation>
									<xs:documentation>
										Element for restart data.
									</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="tsd"
											maxOccurs="unbounded">
											<xs:annotation>
												<xs:documentation>
													One or more messages
													containing testing
													session data from an
													examinee
													workstation.
												</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence
													maxOccurs="unbounded">
													<xs:element
														ref="error" minOccurs="0" />
													<xs:element
														name="ast" minOccurs="0">
														<xs:annotation>
															<xs:documentation>
																Assessment
																State.
																This
																element
																is only
																required
																for
																session
																monitoring
																and
																restart
																and is
																not
																transmitted
																to the
																TMS.
															</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:attribute
																name="cur_eid" type="xs:string" use="required">
																<xs:annotation>
																	<xs:documentation>
																		Encrypted
																		Object
																		ID
																		from
																		the
																		Object
																		Bank.
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute
																name="rem_sec" type="xs:float" use="optional">
																<xs:annotation>
																	<xs:documentation>
																		Remaining
																		duration
																		in
																		seconds.
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element
														name="ist" minOccurs="0">
														<xs:annotation>
															<xs:documentation>
																Item
																State
															</xs:documentation>
														</xs:annotation>
														<xs:complexType>
															<xs:sequence
																maxOccurs="unbounded">
																<xs:annotation>
																	<xs:documentation>
																		Aggregation
																		of
																		response,
																		outcome
																		and
																		context
																		in
																		any
																		order.
																	</xs:documentation>
																</xs:annotation>
																<xs:element
																	name="rv" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>
																			Response
																			Variable
																			with
																			single,
																			multiple
																			or
																			ordered
																			cardinality.
																			For
																			base
																			type
																			of
																			"identifier",
																			the
																			text
																			of
																			the
																			children
																			value
																			elements
																			is
																			set
																			to
																			the
																			identifier
																			of
																			the
																			choice
																			selected
																			by
																			the
																			examinee.
																			For
																			base
																			type
																			of
																			"string",
																			"integer"
																			or
																			"float",
																			the
																			text
																			of
																			the
																			children
																			value
																			elements
																			is
																			set
																			to
																			the
																			value
																			of
																			the
																			student
																			response.
																			Strings
																			are
																			escaped
																			using
																			CDATA
																			or
																			individually
																			escaping
																			the
																			characters.
																		</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element
																				name="v" type="xs:string" />
																		</xs:sequence>
																		<xs:attribute
																			name="n" type="xs:token" use="required"
																			fixed="RESPONSE">
																			<xs:annotation>
																				<xs:documentation>
																					Name
																				</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																		<xs:attribute
																			name="t" type="baseType" use="optional"
																			default="identifier">
																			<xs:annotation>
																				<xs:documentation>
																					baseType
																					from
																					QTI
																				</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																		<xs:attribute
																			name="c" type="cardinalityType" use="optional"
																			default="s">
																			<xs:annotation>
																				<xs:documentation>
																					Cardinality.
																				</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																	</xs:complexType>
																</xs:element>
																<xs:element
																	name="ov" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>
																			Outcome
																			Variable
																		</xs:documentation>
																	</xs:annotation>
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element
																				name="v" type="xs:string" />
																		</xs:sequence>
																		<xs:attribute
																			name="n" type="xs:token" use="required"
																			fixed="SCORE">
																			<xs:annotation>
																				<xs:documentation>
																					Name
																					Common
																					variable:
																					SCORE
																				</xs:documentation>
																			</xs:annotation>
																		</xs:attribute>
																		<xs:attribute
																			name="t" type="baseType" use="optional"
																			default="integer" />
																		<xs:attribute
																			name="c" type="cardinalityType" use="optional"
																			default="s" />
																	</xs:complexType>
																</xs:element>
																<xs:element
																	name="isc" minOccurs="0">
																	<xs:annotation>
																		<xs:documentation>
																			Item
																			Session
																			Context.
																			This
																			element
																			is
																			required
																			only
																			for
																			the
																			item
																			interaction
																			and
																			restart
																			of
																			an
																			item
																			and
																			is
																			not
																			transmitted
																			to
																			the
																			TMS
																		</xs:documentation>
																	</xs:annotation>
																	<xs:complexType />
																</xs:element>
															</xs:sequence>
															<xs:attribute
																name="iid" type="xs:string" use="required">
																<xs:annotation>
																	<xs:documentation>
																		Item
																		ID
																		from
																		QTI
																		Item
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute
																name="eid" type="xs:string" use="required">
																<xs:annotation>
																	<xs:documentation>
																		Encrypted
																		Object
																		ID
																		from
																		the
																		Object
																		Bank.
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute
																name="cst" use="required">
																<xs:annotation>
																	<xs:documentation>
																		completion_status
																		allowed
																		values:
																		not_attempted,
																		completed,
																		incomplete,
																		and
																		unknown
																	</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction
																		base="xs:string">
																		<xs:enumeration
																			value="not_attempted" />
																		<xs:enumeration
																			value="completed" />
																		<xs:enumeration
																			value="incomplete" />
																		<xs:enumeration
																			value="unknown" />
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attribute
																name="dur" type="xs:int" use="optional">
																<xs:annotation>
																	<xs:documentation>
																		Duration
																		in
																		seconds.
																		Cumulative
																		time
																		across
																		multiple
																		attempts
																		of
																		the
																		item.
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute
																name="mrk" type="xs:string" use="optional"
																default="0">
																<xs:annotation>
																	<xs:documentation>
																		Marked
																		for
																		Review.
																		0=false,
																		1=true.
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
															<xs:attribute
																name="awd" type="xs:string" use="optional"
																default="0">
																<xs:annotation>
																	<xs:documentation>
																		Answered.
																		0=false,
																		1=true.
																	</xs:documentation>
																</xs:annotation>
															</xs:attribute>
														</xs:complexType>
													</xs:element>
													<xs:element
														name="sp" type="xs:string" minOccurs="0">
														<xs:annotation>
															<xs:documentation>
																Current
																subtest
																scratchpad
																contents
															</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
												<xs:attribute
													name="lsid" type="xs:string" use="required" />
												<xs:attribute
													name="scid" type="xs:string" use="required" />
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="branding">
								<xs:complexType>
									<xs:attribute name="tdclogo"
										type="xs:string" use="required" />
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="lsid" type="xs:string"
							use="required">
							<xs:annotation>
								<xs:documentation>
									LMS testing session ID supplied by
									the LMS at the start of a testing
									session. This MUST be the same for
									multiple attempts of a test i.e.
									across all restarts of a test and/or
									multiple logins to the LMS.
								</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="laid" type="xs:string"
							use="optional">
							<xs:annotation>
								<xs:documentation>
									Optional LMS Attempt ID supplied by
									the LMS at the start of a testing
									session and at every subsequent
									attempt due to multiple restarts or
									logins. This MAY change across
									multiple logins to the LMS. The
									value is not interpreted or used by
									the assessment delivery system, and
									may be used by the LMS to associate
									the enclosed data with the LMS
									internal data structure.
								</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="restart_flag"
							type="xs:boolean" use="optional" default="0" />
						<xs:attribute name="restart_number"
							type="xs:integer" use="optional" default="0" />
						<xs:attribute
							name="random_distractor_seed_number" type="xs:integer"
							use="optional" default="0" />
						<xs:attribute name="tts_speed_value"
							type="xs:string" use="optional" />
					</xs:complexType>
				</xs:element>

				<xs:element name="run_load_response">
					<xs:complexType>
						<xs:attribute name="system_id"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="roster_id"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="login_id"
							type="xs:string">
						</xs:attribute>

						<xs:attribute name="password"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="access_code"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="run_date"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="status" type="xs:string">
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="uploadStatistics_response">
					<xs:complexType>

						<xs:attribute name="system_id"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="roster_id"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="status" type="xs:string">
						</xs:attribute>
					</xs:complexType>
				</xs:element>
				<xs:element name="upload_systemInfo_response">
					<xs:complexType>
						<xs:attribute name="system_id"
							type="xs:string">
						</xs:attribute>
						<xs:attribute name="status"
							type="xs:string">
						</xs:attribute>
					</xs:complexType></xs:element>
			</xs:sequence>
			<xs:attribute name="method" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="entryType">
		<xs:annotation>
			<xs:documentation>An indication  of whether the student has been in the SCO before</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ab-initio">
				<xs:annotation>
					<xs:documentation>Indicates that it is first time the student is entering the SCO</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="resume">
				<xs:annotation>
					<xs:documentation>Indicates that the student was in the SCO earlier</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="">
				<xs:annotation>
					<xs:documentation>Indicate the entry of a SCO that was already completed. </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:element name="stereotype_style">
		<xs:complexType>
			<xs:attribute name="stereotype" type="stereotypeType" use="required"/>
			<xs:attribute name="bgcolor" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Background color for this stereotype.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="fontcolor" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Font color for this stereotype.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>

	<xs:complexType name="run_load_requestType">
		<xs:attribute name="system_id" type="xs:string"></xs:attribute>
		<xs:attribute name="system_date" type="xs:dateTime"></xs:attribute>
	</xs:complexType>

	<xs:complexType name="run_load_responseType">
		<xs:attribute name="system_id" type="xs:string"></xs:attribute>
		<xs:attribute name="roster_id" type="xs:string"></xs:attribute>
		<xs:attribute name="login_id" type="xs:string"></xs:attribute>
		<xs:attribute name="password" type="xs:string"></xs:attribute>

		<xs:attribute name="access_code" type="xs:string"></xs:attribute>
		<xs:attribute name="run_time" type="xs:dateTime"></xs:attribute>
	</xs:complexType>

	<xs:attribute name="NewAttribute" type="xs:string"></xs:attribute>

</xs:schema>
