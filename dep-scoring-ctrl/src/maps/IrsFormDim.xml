<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="IRSFormDim">

<select id="findByFormID" parameterClass="com.ctb.lexington.db.irsdata.IrsFormDimData" resultClass="com.ctb.lexington.db.irsdata.IrsFormDimData">
SELECT 
        FORMID as formid,
        NAME as name
FROM
        FORM_DIM
WHERE 
        FORMID = #formid:NUMERIC#
</select>
<select id="findByFormNameProductTypeId" parameterClass="com.ctb.lexington.db.irsdata.IrsFormDimData" resultClass="com.ctb.lexington.db.irsdata.IrsFormDimData">
SELECT 
        FORMID as formid,
        NAME as name,
        PRODUCT_TYPEID as productTypeId
FROM
        FORM_DIM
WHERE 
        NAME = #name:VARCHAR#
AND		PRODUCT_TYPEID = #productTypeId:NUMERIC#
</select>
<insert id="insertFormDim" parameterClass="com.ctb.lexington.db.irsdata.IrsFormDimData">
    INSERT INTO
        FORM_DIM
                (
                FORMID,
                NAME
                )
        VALUES
                (
                #formid:NUMERIC#,
                #name:VARCHAR#
                )        
</insert>
<update id="updateFormDim" parameterClass="com.ctb.lexington.db.irsdata.IrsFormDimData">
        UPDATE
                FORM_DIM
        SET
            NAME = #name:VARCHAR#        
        WHERE
           FORMID = #formid:NUMERIC#
</update>
<delete id="deleteFormDim" parameterClass="java.lang.Long">

	DELETE FROM
			FORM_DIM
	WHERE	
			FORMID = #formid:NUMERIC#
</delete>
</sqlMap>