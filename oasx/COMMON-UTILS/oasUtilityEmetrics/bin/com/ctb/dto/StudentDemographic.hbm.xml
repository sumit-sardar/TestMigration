<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ctb.dto">
	<class name="StudentDemographic" table="student_demographic_data">
		<id name="studentDemographicId" type="int" column="STUDENT_DEMOGRAPHIC_DATA_ID">
			<generator class="assigned" />
		</id>

		<property name="customerDemographicId">
			<column name="CUSTOMER_DEMOGRAPHIC_ID" />
		</property>
		<property name="valueName">
			<column name="VALUE_NAME" />
		</property>
		
		
				
	</class>
</hibernate-mapping>